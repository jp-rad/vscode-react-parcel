{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "parcel: build",
            "type": "shell",
            "command": "yarn",
            "args": [
                "build",
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            },
        },
        {
            "label": "yarn: install",
            "type": "shell",
            "command": "yarn",
            "args": [
                "install",
                "--immutable",
                "--immutable-cache",
                "--check-cache",
            ],
        },
    ]
}